<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!--Title-->
    <title>Titan | @ViewBag.Title</title>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>

    

    <!-- Primary Styles -->
    @Styles.Render("~/bundles/bootstrap/css")
    @Styles.Render("~/bundles/homer/css")
    @Styles.Render("~/bundles/font-awesome/css")

    <!-- Nigmys Styles -->
    @Styles.Render("~/bundle/main/css")

    <!-- Add local styles, mostly for plugins css file -->
    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}
    
</head>
<body>
    <!--Main Header-->
    @Html.Partial("WebLayouts/PartialLayouts/_Register_Header")
    <article class="main-background"></article>

    <!--Main Body-->
    @RenderBody()


    <!-- Section for main scripts render -->

    @RenderSection("scripts", required: false)
</body>
</html>
